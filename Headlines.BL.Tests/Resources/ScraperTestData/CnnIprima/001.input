    function onSSOLoad() {
        // user is logged out
        sso.listen('sso.anonymousUser', function () {
            ssoInitialized = true;
            onLogout();
        });

        // user is logged in
        sso.listen('sso.tokenReceived', function () {
            ssoInitialized = true;
            onLogin();
        });
    }

    /**
     */
    function login() {
        sso.login({'lpt': 'login'});
    }

    /**
     */
    function logout() {
        sso.logout();
        onLogout();
    }

    /**
     */
    function getUserInfo() {
        let token = sso.getAccessToken();
        let response = $.ajax({
            url: 'https://auth.iprima.cz/oauth2/UserInfo',
            method: 'get',
            beforeSend: function(request) {
                request.setRequestHeader('Authorization', 'Bearer ' + token);
            },
            async: false
        });

        return response.readyState === 4 ? JSON.parse(response.responseText) : null;
    }

    /**
     */
    function onLogin() {
        let userInfo = getUserInfo();
        let name;

        if (userInfo) {
            // fill in logged user
            if (userInfo.given_name && userInfo.given_name.length > 1
                && userInfo.family_name && userInfo.family_name.length > 1) {
                name = userInfo.given_name + ' ' + userInfo.family_name;
            } else if (userInfo.given_name && userInfo.given_name.length > 1) {
                name = userInfo.given_name;
            } else if (userInfo.family_name && userInfo.family_name.length > 1) {
                name = userInfo.family_name;
            } else if (userInfo.name && userInfo.name.length > 1) {
                name = userInfo.name;
            } else if (userInfo.email && userInfo.email.length > 1) {
                name = userInfo.email;
            }

            $('#login-account').find('div.login-item-description').html(escapeHtml(name));
            $('#login-mobile-user').text(escapeHtml(name));
            // show/hide elements
            $('#logged-out-submenu').addClass('hidden');
            $('#logged-in-submenu').removeClass('hidden');
            $('.only-logged-out').addClass('hidden');
            $('.only-logged-in').removeClass('hidden');

            setUserIcon(true);
        }
    }

    /**
     */
    function onLogout() {
        $('#logged-out-submenu').removeClass('hidden');
        $('#logged-in-submenu').addClass('hidden');
        $('.only-logged-in').addClass('hidden');
        $('.only-logged-out').removeClass('hidden');

        setUserIcon(false);
    }

    /**
     */
    function setUserIcon(logged) {
        let userIconElement = $('#user-login-icon');
        userIconElement.toggleClass('i-user', !logged);
        userIconElement.toggleClass('i-user-filled', logged);
    }
